{
  "name": "nativescript-zendesk-sdk",
  "version": "0.0.3",
  "description": "Zendesk SDK for NativeScript",
  "main": "zendesk-sdk",
  "typings": "zendesk-sdk.d.ts",
  "nativescript": {
    "platforms": {
      "android": "3.0.0",
      "ios": "3.0.0"
    }
  },
  "scripts": {
    "tns": "tns",
    "tsc": "tsc",
    "tslint": "tslint",
    "lint": "tslint *.ts --type-check --force --fix",
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd demo && tns run ios --emulator",
    "demo.ios.device": "npm run preparedemo && cd demo && tns run ios",
    "demo.android": "npm run preparedemo && cd demo && tns run android --emulator",
    "test.ios": "cd demo && tns test ios --emulator",
    "test.ios.device": "cd demo && tns test ios",
    "test.android": "cd demo && tns test android",
    "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-zendesk-sdk && tns plugin add .. && tns install",
    "setup": "npm i && cd demo && npm i && cd .. && npm run build && cd demo && tns plugin add .. && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DickSmith/nativescript-zendesk-sdk.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS"
  ],
  "author": {
    "name": "Dick Smith",
    "email": "richard.smith@ugroupmedia.com"
  },
  "bugs": {
    "url": "https://github.com/DickSmith/nativescript-zendesk-sdk/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/DickSmith/nativescript-zendesk-sdk",
  "readmeFilename": "README.md",
  "devDependencies": {
    "nativescript": "^3.0.3",
    "prompt": "^1.0.0",
    "rimraf": "^2.5.0",
    "tns-core-modules": "^3.0.1",
    "tns-platform-declarations": "^3.0.1",
    "tslint": "^5.4.3",
    "typescript": "^2.4.0"
  }
}
